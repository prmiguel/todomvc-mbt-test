{
  "name": "TodoMVCTestSuite",
  "models": [
    {
      "id": "TodoMVC_Core_Model",
      "name": "TodoMVC_Core_Model",
      "startElementId": "v_Start",
      "generator": "random(edge_coverage(100))",
      "vertices": [
        {
          "id": "v_Start",
          "name": "v_Start",
          "properties": {
            "description": "Initial state: The application is launched."
          }
        },
        {
          "id": "v_EmptyList",
          "name": "v_EmptyList",
          "properties": {
            "description": "State where the todo list has 0 items."
          }
        },
        {
          "id": "v_ActiveTodos",
          "name": "v_ActiveTodos",
          "properties": {
            "description": "State with one or more active (incomplete) items."
          }
        },
        {
          "id": "v_CompletedTodos",
          "name": "v_CompletedTodos",
          "properties": {
            "description": "State with one or more completed items."
          }
        }
      ],
      "edges": [
        {
          "id": "e_startApp",
          "name": "e_startApp",
          "sourceVertexId": "v_Start",
          "targetVertexId": "v_EmptyList"
        },
        {
          "id": "e_addTodo",
          "name": "e_addTodo",
          "sourceVertexId": "v_EmptyList",
          "targetVertexId": "v_ActiveTodos"
        },
        {
          "id": "e_toggleTodo",
          "name": "e_toggleTodo",
          "sourceVertexId": "v_ActiveTodos",
          "targetVertexId": "v_CompletedTodos"
        },
        {
          "id": "e_clearCompleted",
          "name": "e_clearCompleted",
          "sourceVertexId": "v_CompletedTodos",
          "targetVertexId": "v_EmptyList"
        },
        {
          "id": "e_updateTodo",
          "name": "e_updateTodo",
          "sourceVertexId": "v_ActiveTodos",
          "targetVertexId": "v_ActiveTodos"
        },
        {
          "id": "e_addAnotherTodo",
          "name": "e_addAnotherTodo",
          "sourceVertexId": "v_ActiveTodos",
          "targetVertexId": "v_ActiveTodos"
        },
        {
          "id": "e_uncompleteTodo",
          "name": "e_uncompleteTodo",
          "sourceVertexId": "v_CompletedTodos",
          "targetVertexId": "v_ActiveTodos"
        },
        {
          "id": "e_deleteTodo",
          "name": "e_deleteTodo",
          "sourceVertexId": "v_ActiveTodos",
          "targetVertexId": "v_EmptyList",
          "guard": "numberOfActiveTodos == 1"
        },
        {
          "id": "e_deleteTodo_Loop",
          "name": "e_deleteTodo_Loop",
          "sourceVertexId": "v_ActiveTodos",
          "targetVertexId": "v_ActiveTodos",
          "guard": "numberOfActiveTodos > 1"
        },
        {
          "id": "e_deleteCompletedTodo",
          "name": "e_deleteCompletedTodo",
          "sourceVertexId": "v_CompletedTodos",
          "targetVertexId": "v_EmptyList",
          "guard": "numberOfCompletedTodos == 1"
        }
      ]
    }
  ]
}